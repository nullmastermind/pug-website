  body
    .boxed_wrapper
      //  preloader 
      .loader-wrap
        .preloader
          #handle-preloader.handle-preloader
            .animation-preloader
              .spinner
              .txt-loading
                span.letters-loading(data-text-preloader="h") h
                span.letters-loading(data-text-preloader="ộ") ộ
                span.letters-loading(data-text-preloader="i") i
                span.letters-loading(data-text-preloader=" ")
                span.letters-loading(data-text-preloader="a") a
                span.letters-loading(data-text-preloader="n") n
                span.letters-loading(data-text-preloader=" ")
                span.letters-loading(data-text-preloader="d") d
                span.letters-loading(data-text-preloader="'") '
                span.letters-loading(data-text-preloader="o") o
                span.letters-loading(data-text-preloader="r") r
      //  preloader end 
      //  main header 
      header.main-header.style-one
        //  header-lower 
        .header-lower
          .auto-container
            .outer-box
              .logo-box
                figure.logo
                  a(href="index.html")
                    img(src="assets/images/logo-dangky-02-02.png", alt="logo du an hoi an dor, hoi an d'or")
              .menu-area.clearfix
                // Mobile Navigation Toggler
                .mobile-nav-toggler
                  i.icon-bar
                  i.icon-bar
                  i.icon-bar
                nav.main-menu.navbar-expand-md.navbar-light
                  #navbarSupportedContent.collapse.navbar-collapse.show.clearfix
                    ul.navigation.clearfix
                      li.current
                        a(href="#") Trang chủ
                      li
                        a(href="#") Dự án
                      li
                        a(href="#") Liên Hệ
        // sticky Header
        .sticky-header
          .auto-container
            .outer-box
              .logo-box
                figure.logo
                  a(href="index.html")
                    img(src="assets/images/logo-dangky-02-02.png", alt)
              .menu-area
                nav.main-menu.clearfix
                  // Keep This Empty / Menu will come through Javascript
              ul.menu-right-content.clearfix
                li.search-box-outer
                  .dropdown
                    button#dropdownMenu4.search-box-btn(type="button", data-toggle="dropdown", aria-haspopup="true", aria-expanded="false")
                      i.far.fa-search
                    .dropdown-menu.search-panel(aria-labelledby="dropdownMenu4")
                      .form-container
                        form(method="post", action="#")
                          .form-group
                            input(type="search", name="search-field", placeholder="Search....", required)
                            button.search-btn(type="submit")
                              span.fas.fa-search
                li.user-link
                  a(href="#")
                    i.icon-Profile
      //  main-header end 
      //  Mobile Menu  
      .mobile-menu
        .menu-backdrop
        .close-btn
          i.fas.fa-times
        nav.menu-box
          .nav-logo
            a(href="index.html")
              img(src="assets/images/logo-dangky-02-02.png", alt)
          .menu-outer
            // Here Menu Will Come Automatically Via Javascript / Same Menu as in Header
          .contact-info
            h4 Contact Info
            ul
              li Chicago 12, Melborne City, USA
              li
                a(href="tel:+8801682648101") +88 01682648101
              li
                a(href="mailto:info@example.com") info@example.com
          .social-links
            ul.clearfix
              li
                a(href="index.html")
                  span.fab.fa-twitter
              li
                a(href="index.html")
                  span.fab.fa-facebook-square
              li
                a(href="index.html")
                  span.fab.fa-pinterest-p
              li
                a(href="index.html")
                  span.fab.fa-instagram
              li
                a(href="index.html")
                  span.fab.fa-youtube
      //  End Mobile Menu 
      //  banner-section 
      section#contact.banner-section(style="background-image: url(assets/images/nh-ban-m-2.jpg)")
        .pattern-layer(style="background-image: url(theme/assets/images/shape/shape-1.png)")
        .auto-container
          .content-box
            h2
              | Dự án
              br
              | Hội An D'Or
            p Dự án nghỉ dưỡng đáng chú ý tại Hội An – Đà Nẵng trong năm 2021
            .form-inner
              form.booking-form.la-contact-form.clearfix(action="index.html", method="post")
                .form-group
                  input(type="text", name="name", placeholder="Họ tên", required)
                .form-group
                  input(type="text", name="email", placeholder="Địa chỉ Email")
                .form-group
                  input(type="text", name="mobile", placeholder="Số điện thoại")
                .message-btn
                  button.theme-btn(type="submit") Nhận thông tin
